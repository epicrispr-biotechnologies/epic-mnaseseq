//Nextflow config file for running on AWS batch
params {
    config_profile_description = 'AWSBATCH Cloud Profile'
    awsqueue                   = 'tyler_gp_queue'
    awsregion                  = 'us-west-2'
}

timeline {
    overwrite = true
}
report {
    overwrite = true
}
trace {
    overwrite = true
}
dag {
    overwrite = true
}

process.executor = 'awsbatch'
process.queue = params.awsqueue
aws.region = params.awsregion
